<%- include("partials/header") %>

    <h1>Home</h1>
    <p>
        <%= homeStartingContent%>
    </p>
    <P>
    <div class="box">
        <%posts.forEach((post)=> { %>
            <%console.log(post.postTitle);%>
                <div class="postTitle">
                    <h2>
                        <%=post.postTitle%>
                    </h2>
                </div>
                <div class="postBody">
                    <p>
                        <%=post.postBody.substring(1, 100) + " ..." %>
                            <a href="/posts/<%=post._id%>">Read More</a>
                    </p>
                </div>
                <%});%>
    </div>
    </P>
    <%- include("partials/footer") %>